<Page x:Class="TestAppWpf.TestCOVID"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TestAppWpf"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="1280"
      Title="TestCOVID">

    <Grid Background="White">
        <ScrollViewer>
            <StackPanel>
                
                <Grid Margin="10,20,10,15">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="60"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <!--Кнопка возврата в общее меню-->
                    <Button x:Name="buttonMainMenuFirst" Grid.Column="0" Grid.Row="0" Content="Вернуться назад" Margin="10, 0, 10, 10" Background="Orange" Click="buttonMainMenu_Clicked"/>
                </Grid>

                <!--Название теста в рамке-->
                <TextBlock FontSize="30" FontWeight="Bold" Background="Orange" TextAlignment="Center ">
                        Тест COVID-19
                </TextBlock>

                <!--Первый вопрос-->
                <TextBlock Text="1) Пол:" Foreground="Black" FontSize="20" Margin="15,0"/>
                <RadioButton x:Name="radioButtonGender1" GroupName="Gender" Margin="15,0" Checked="radioButtonGender_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonGender2" GroupName="Gender" Margin="15,0" Checked="radioButtonGender_Checked" FontSize="20" VerticalContentAlignment="Center"/>

                <!--Второй вопрос-->
                <TextBlock Text="2) Ваш уровень образования:" Foreground="Black" FontSize="20" Margin="15,25,15,0"/>
                <RadioButton x:Name="radioButtonEducation1" GroupName="Education" Margin="15,0" Checked="radioButtonEducation_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonEducation2" GroupName="Education" Margin="15,0" Checked="radioButtonEducation_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonEducation3" GroupName="Education" Margin="15,0" Checked="radioButtonEducation_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonEducation4" GroupName="Education" Margin="15,0" Checked="radioButtonEducation_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonEducation5" GroupName="Education" Margin="15,0" Checked="radioButtonEducation_Checked" FontSize="20" VerticalContentAlignment="Center"/>

                <!--Третий вопрос-->
                <TextBlock Text="3) Ваша профессиональная деятельность:" Foreground="Black" FontSize="20" Margin="15,25,15,0"/>
                <CheckBox x:Name="checkBoxActivity1" IsThreeState="False" Checked="checkBoxActivity_Checked" Unchecked="checkBoxActivity_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxActivity2" IsThreeState="False" Checked="checkBoxActivity_Checked" Unchecked="checkBoxActivity_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxActivity3" IsThreeState="False" Checked="checkBoxActivity_Checked" Unchecked="checkBoxActivity_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxActivity4" IsThreeState="False" Checked="checkBoxActivity_Checked" Unchecked="checkBoxActivity_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxActivity5" IsThreeState="False" Checked="checkBoxActivity_Checked" Unchecked="checkBoxActivity_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxActivity6" IsThreeState="False" Checked="checkBoxActivity_Checked" Unchecked="checkBoxActivity_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>

                <!--Четвертый вопрос-->
                <TextBlock Text="4) Были ли у Вас диагностированы какие-либо из следующих состояний до короновирусного заболевания?" Foreground="Black" FontSize="20" Margin="15,25,15,0"/>
                <CheckBox x:Name="checkBoxDiseases1" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases2" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases3" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases4" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases5" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases6" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases7" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases8" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases9" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>
                <CheckBox x:Name="checkBoxDiseases10" IsThreeState="False" Checked="checkBoxDiseases_Checked" Unchecked="checkBoxDiseases_Unchecked" Margin="15,5" FontSize="20" VerticalContentAlignment="Center"/>

                <!--Пятый вопрос-->
                <TextBlock Text="5) Изменилась ли Ваша трудоспособность после перенесенной коронавирусной инфекции?" Foreground="Black" FontSize="20" Margin="15,25,15,0"/>
                <RadioButton x:Name="radioButtonAbilityToWork1" GroupName="AbilityToWork" Margin="15,0" Checked="radioButtonAbilityToWork_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonAbilityToWork2" GroupName="AbilityToWork" Margin="15,0" Checked="radioButtonAbilityToWork_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonAbilityToWork3" GroupName="AbilityToWork" Margin="15,0" Checked="radioButtonAbilityToWork_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <Grid Margin="30,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="2*"/>
                    </Grid.ColumnDefinitions>
                    <Label x:Name="labelAbilityToWork" Content="На сколько ?" Foreground="Gray" FontSize="18" VerticalAlignment="Center" Grid.Column="0"/>
                    <Slider x:Name="sliderAbilityToWork" Maximum="10.49" Minimum="0.5" Foreground="Orange" ValueChanged="slider_ValueChanged" VerticalAlignment="Center" Grid.Column="1"/>
                </Grid>

                <!--Шестой вопрос-->
                <TextBlock Text="6) Изменилась ли Ваша работоспособность по дому после перенесенной коронавирусной инфекции?" Foreground="Black" FontSize="20" Margin="15,25,15,0"/>
                <RadioButton x:Name="radioButtonPerfomance1" GroupName="Perfomance" Margin="15,0" Checked="radioButtonPerfomance_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonPerfomance2" GroupName="Perfomance" Margin="15,0" Checked="radioButtonPerfomance_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonPerfomance3" GroupName="Perfomance" Margin="15,0" Checked="radioButtonPerfomance_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <Grid Margin="30,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="2*"/>
                    </Grid.ColumnDefinitions>
                    <Label x:Name="labelPerformance" Content="На сколько ?" Foreground="Gray" FontSize="18" VerticalAlignment="Center" Grid.Column="0"/>
                    <Slider x:Name="sliderPerformance" Maximum="10.49" Minimum="0.5" Foreground="Orange" ValueChanged="slider_ValueChanged" VerticalAlignment="Center" Grid.Column="1"/>
                </Grid>

                <!--Седьмой вопрос-->
                <TextBlock Text="7) Изменилась ли Ваша досуговая активность после перенесенной коронавирусной инфекции?" Foreground="Black" FontSize="20" Margin="15,25,15,0"/>
                <RadioButton x:Name="radioButtonLeisure1" GroupName="Leisure" Margin="15,0" Checked="radioButtonLeisure_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonLeisure2" GroupName="Leisure" Margin="15,0" Checked="radioButtonLeisure_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <RadioButton x:Name="radioButtonLeisure3" GroupName="Leisure" Margin="15,0" Checked="radioButtonLeisure_Checked" FontSize="20" VerticalContentAlignment="Center"/>
                <Grid Margin="30,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="2*"/>
                    </Grid.ColumnDefinitions>
                    <Label x:Name="labelLeisure" Content="На сколько ?" Foreground="Gray" FontSize="18" VerticalAlignment="Center" Grid.Column="0"/>
                    <Slider x:Name="sliderLeisure" Maximum="10.49" Minimum="0.5" Foreground="Orange" ValueChanged="slider_ValueChanged" VerticalAlignment="Center" Grid.Column="1"/>
                </Grid>


                <!--Таблица жалоб-->
                <Grid Background="White" Margin="4,80,4,35">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="50"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>


                    <!--Заголовок таблицы-->
                    <Border Grid.Column="0" Grid.Row="0" BorderThickness="2,2,1,1" BorderBrush="Black" Background="Orange"/>
                    <TextBlock TextWrapping="Wrap" Text="Жалобы" Foreground="Black" FontSize="22" VerticalAlignment="Center" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" FontWeight="Bold"/>
                    <Border Grid.Column="1" Grid.Row="0" BorderThickness="1,2,1,1" BorderBrush="Black" Background="Orange"/>
                    <TextBlock TextWrapping="Wrap" Text="При поступлении" Foreground="Black" FontSize="22" VerticalAlignment="Center" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" FontWeight="Bold"/>
                    <Border Grid.Column="2" Grid.Row="0" BorderThickness="1,2,1,1" BorderBrush="Black" Background="Orange"/>
                    <TextBlock TextWrapping="Wrap" Text="При выписке" Foreground="Black" FontSize="22" VerticalAlignment="Center" Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" FontWeight="Bold"/>
                    <Border Grid.Column="3" Grid.Row="0" BorderThickness="1,2,2,1" BorderBrush="Black" Background="Orange"/>
                    <TextBlock TextWrapping="Wrap" Text="Через 3 месяца после выписки" Foreground="Black" FontSize="22" VerticalAlignment="Center" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Center" FontWeight="Bold"/>


                    <!--Подзаголовки таблицы таблицы-->
                    <Border Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Неврологические" Foreground="Black" FontSize="20" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="1" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="9" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Головная боль" Foreground="Black" FontSize="20" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="9" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="13" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Аффективные расстройства" Foreground="Black" FontSize="20" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="13" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="18" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Системные проявления" Foreground="Black" FontSize="20" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="18" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="23" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Сердечно-сосудистая система" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="23" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="30" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Опорно-двигательный аппарат" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="30" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="33" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Органы чувств" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="33" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="43" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Дыхательная система" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="43" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="50" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Лор" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="50" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="56" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Желудочно-кишечный тракт" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="56" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>
                    <Border Grid.Column="0" Grid.Row="60" Grid.ColumnSpan="4" BorderThickness="2,1" BorderBrush="Black" Background="#ffb02e"/>
                    <Label Content="Дерматологические проблемы" Foreground="Black" FontSize="18" VerticalContentAlignment="Center" Grid.Column="0" Grid.ColumnSpan="4" Grid.Row="60" HorizontalContentAlignment="Center" FontWeight="Bold" Height="40"/>


                    <!--Блок неврологических жалоб-->
                    <Border Grid.Column="0" Grid.Row="2" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Кратковременная потеря памяти" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="2" Margin="3,0"/>
                    <TextBox x:Name="editorShortTermMemoryLoss1" Foreground="Black" Grid.Column="1" Grid.Row="2" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorShortTermMemoryLoss_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorShortTermMemoryLoss2" Foreground="Black" Grid.Column="2" Grid.Row="2" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorShortTermMemoryLoss_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorShortTermMemoryLoss3" Foreground="Black" Grid.Column="3" Grid.Row="2" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorShortTermMemoryLoss_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"  BorderThickness="1,1,2,1"/>

                    <Border Grid.Column="0" Grid.Row="3" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Снижение памяти" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="3" Margin="3,0"/>
                    <TextBox x:Name="editorDecreasedMemory1" Foreground="Black" Grid.Column="1" Grid.Row="3" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorDecreasedMemory_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorDecreasedMemory2" Foreground="Black" Grid.Column="2" Grid.Row="3" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorDecreasedMemory_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorDecreasedMemory3" Foreground="Black" Grid.Column="3" Grid.Row="3" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorDecreasedMemory_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"  BorderThickness="1,1,2,1"/>

                    <Border Grid.Column="0" Grid.Row="4" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Снижение концентрации внимания, 'туман в мозгу'" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="4" Margin="3,0"/>
                    <TextBox x:Name="editorDecreasedConcentration1" Foreground="Black" Grid.Column="1" Grid.Row="4" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorDecreasedConcentration_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorDecreasedConcentration2" Foreground="Black" Grid.Column="2" Grid.Row="4" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorDecreasedConcentration_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorDecreasedConcentration3" Foreground="Black" Grid.Column="3" Grid.Row="4" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorDecreasedConcentration_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"  BorderThickness="1,1,2,1"/>

                    <Border Grid.Column="0" Grid.Row="5" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Повышенная утомляемость" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="5" Margin="3,0"/>
                    <TextBox x:Name="editorFatigue1" Foreground="Black" Grid.Column="1" Grid.Row="5" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorFatigue_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorFatigue2" Foreground="Black" Grid.Column="2" Grid.Row="5" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorFatigue_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorFatigue3" Foreground="Black" Grid.Column="3" Grid.Row="5" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorFatigue_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"  BorderThickness="1,1,2,1"/>

                    <Border Grid.Column="0" Grid.Row="6" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Спутанность сознания" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="6" Margin="3,0"/>
                    <TextBox x:Name="editorConfusion1" Foreground="Black" Grid.Column="1" Grid.Row="6" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorConfusion_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorConfusion2" Foreground="Black" Grid.Column="2" Grid.Row="6" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorConfusion_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorConfusion3" Foreground="Black" Grid.Column="3" Grid.Row="6" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorConfusion_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"  BorderThickness="1,1,2,1"/>

                    <Border Grid.Column="0" Grid.Row="7" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Нарушение сна (патологическое бодрствование, бессонница)" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="7" Margin="3,0"/>
                    <TextBox x:Name="editorSleepDisturbance1" Foreground="Black" Grid.Column="1" Grid.Row="7" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorSleepDisturbance_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorSleepDisturbance2" Foreground="Black" Grid.Column="2" Grid.Row="7" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorSleepDisturbance_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"/>
                    <TextBox x:Name="editorSleepDisturbance3" Foreground="Black" Grid.Column="3" Grid.Row="7" SpellCheck.IsEnabled="True" Language="ru-ru" TextChanged="editorSleepDisturbance_TextChanged" AcceptsReturn="True" TextWrapping="Wrap" FontSize="18" BorderBrush="Black"  BorderThickness="1,1,2,1"/>

                    <Border Grid.Column="0" Grid.Row="8" BorderThickness="2,1,1,1" BorderBrush="Black"/>
                    <TextBlock TextWrapping="Wrap" Text="Проблемы с речью (подбор слов или произношение)" Foreground="Black" FontSize="18" VerticalAlignment="Center" Grid.Column="0" Grid.Row="8" Margin="3,0"/>
